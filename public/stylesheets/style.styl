*
  margin: 0
  padding: 0
  text-decoration: none
  font-size: 1em
  outline: none
a
  color: #00B7FF
#wrapper
  margin: 10px 0 0 10px
  width: 1100px
#header
  width: 250px
  position: relative
  float: left
  margin-right: 10px
  clear: both
  z-index: 6
  h1
    height: 148px
    border: 1px solid #066
    border-width: 1px 0
    position: relative
    font-size: 24px
    a
      position: relative
      display: block
      height 150px
      color: #003
      span
        bottom: 14px
        position: absolute
        padding: 0 10px
        line-height: 1em
        text-decoration: none 
#nav
  border-top: 1px solid #066
  border-bottom: 1px solid #AAA
  height: 70px
  padding: 0
  margin: 0
  float: left
  width: 800px
  color: #003
  li
    list-style: none
    padding: 6px 0
    a
      border-right: 1px solid #DDD 
      padding: 0 0 6px 4px
      height: 47px
      width: 85px
      color: #003
#submenu
   height: 80px
   border-top: 1px solid #066
   border-bottom: 1px solid #066
