  body
    #wrapper
      if (userinfo != null)
        #mobileHeader
          #mobileHeaderContent.invisible
            include timemobile
      block leftheader
        #header
          #logo
            h1
              img(src="/images/m3_logo.png") 
          if (userinfo != null)
            #mobileMenu
              p 
                a(href="#") Add Class
      #nav
        #date 2012
        #line1 M3 Conference
        #line2 Class Scheduler
      #content
